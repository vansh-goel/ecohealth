#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vanshgoel/Downloads/ecohealth-react/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+plugin-transform-react-jsx@7.22.1_7rweoznqqsvkrlvrvqmssetxf4/node_modules/react-scripts/bin/node_modules:/home/vanshgoel/Downloads/ecohealth-react/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+plugin-transform-react-jsx@7.22.1_7rweoznqqsvkrlvrvqmssetxf4/node_modules/react-scripts/node_modules:/home/vanshgoel/Downloads/ecohealth-react/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+plugin-transform-react-jsx@7.22.1_7rweoznqqsvkrlvrvqmssetxf4/node_modules:/home/vanshgoel/Downloads/ecohealth-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vanshgoel/Downloads/ecohealth-react/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+plugin-transform-react-jsx@7.22.1_7rweoznqqsvkrlvrvqmssetxf4/node_modules/react-scripts/bin/node_modules:/home/vanshgoel/Downloads/ecohealth-react/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+plugin-transform-react-jsx@7.22.1_7rweoznqqsvkrlvrvqmssetxf4/node_modules/react-scripts/node_modules:/home/vanshgoel/Downloads/ecohealth-react/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+plugin-transform-react-jsx@7.22.1_7rweoznqqsvkrlvrvqmssetxf4/node_modules:/home/vanshgoel/Downloads/ecohealth-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
fi
